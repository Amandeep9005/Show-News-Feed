{
  "name": "news-app",
  "version": "0.1.0",
  "private": true,
  "homepage": "/news",
  "dependencies": {
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "enzyme-adapter-react-16": "^1.15.6",
    "immer": "^9.0.2",
    "jest-environment-jsdom-sixteen": "^2.0.0",
    "jest-sonar-reporter": "^2.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "redux": "^4.1.0",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.3.0",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "npm run test:unit",
    "eject": "react-scripts eject",
    "test:unit": "react-scripts test --env jest-environment-jsdom-sixteen --coverage --coverageDirectory=./coverage --testResultsProcessor ./node_modules/jest-sonar-reporter/index.js --testMatch=**/*.unit.test.js --runInBand --watchAll=false"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },

  "jest": {

      "collectCoverageFrom": [

          "src/**/*.{js,jsx}",

          "!**/node_modules/**",

          "!**/vendor/**",

          "!src/serviceWorker.js",

          "!src/index.js",

          "!src/Component/StateSetup/GetInitialState.js",

          "!src/setupTests.js",
          "!src/App.js",
          "!src/App.test.js",
          "!src/reportWebVitals.js"

      ]

  }
}
